{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "removeComments": true,
        "sourceMap": true,
        "jsx": "react-jsx",
    },
    "include": [
        "src/**/*.ts",
        "src/index.tsx",
    ],
    "exclude": [
        "node_modules",
    ],
}